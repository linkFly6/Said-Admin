:global(.saidiconfont)
  display inline-block
  font-style normal
  vertical-align baseline
  text-align center
  text-transform none
  line-height 1
  text-rendering optimizeLegibility
  -webkit-font-smoothing antialiased
  min-width 14px
  margin-right: 8px
  transition all .3s
  transition all .3s
  &:before
    display block